<h1>Edit camera</h1>
<%= form_for @camera do |f| %>

    <p><%= f.label :Model_make %> <br/>
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.label :description %> <br/>
    <%= f.text_area :description %>
  </p>


  <p><%= f.label :User %> <br/>
  <%= f.collection_select :user_id, User.all, :id, :name %>
</p>
  <%=f.submit'Edit camera'%> 

  <%end%>


  <%=link_to "Add pics", new_photo_path(@camera)%>
  <%=link_to "Go back to home page", cameras_path%>